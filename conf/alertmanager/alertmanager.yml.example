route:
  receiver: "default"
  group_by: ["alertname", "service"]
  group_wait: 10s
  group_interval: 2m
  repeat_interval: 30m

receivers:
  - name: "default"
    webhook_configs:
      - url: "http://webhook-receiver:9093/"
        send_resolved: true

inhibit_rules:
  - source_match:
      severity: "critical"
    target_match:
      severity: "warning"
    equal: ["alertname", "service"]
