user default on >${REDIS_PASSWORD} ~* &* +@all -@dangerous
user searxng on >${REDIS_PASSWORD} ~* &* +@read +@write +@pubsub +@connection +@string +@hash +@list +@set +@sortedset -@dangerous -@admin
user openwebui on >${REDIS_PASSWORD} ~* &* +@read +@write +@pubsub +@connection +@string +@hash +@transaction -@dangerous -@admin
user litellm on >${REDIS_PASSWORD} ~litellm:* &* +@read +@write +@connection +@string +@hash -@dangerous -@admin
user exporter on >${REDIS_PASSWORD} ~* &* +@read +INFO +SLOWLOG +CLIENT|LIST +CONFIG|GET -@write -@dangerous -@admin
